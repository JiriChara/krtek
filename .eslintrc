{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "no-plusplus": 0,
    "no-bitwise": 0,
    "func-names": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/no-danger": 0
  }
}
